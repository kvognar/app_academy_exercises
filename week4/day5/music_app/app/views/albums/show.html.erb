<h1><%= @album.title %></h1>
<%= link_to @album.band.name, band_url(@album.band) %> <br>
<%= link_to "Edit", edit_album_url(@album) %>

<h3>Tracks</h3>
<ul>
  <% @album.tracks.each do |track| %> 
    <li><%= link_to track.title, track_url(track) %></li>
  <% end %>
</ul>
<%= link_to "Add track", new_album_track_url(@album) %>
<%= button_to "Delete Album", album_url(@album),
   method: :delete, data: { confirm: "Are you sure?" } %>
